{
  "name": "thanatsitt-portfolio",
  "version": "1.0.0",
  "description": "Thanatsitt Santisamranwilai - Cultural Navigator Portfolio",
  "scripts": {
    "build": "node scripts/build.js",
    "build:dev": "NODE_ENV=development node scripts/build.js",
    "build:prod": "NODE_ENV=production node scripts/build.js",
    "dev": "npm run build:dev && netlify dev",
    "dev:local": "npm run build:dev && live-server dist --port=3000",
    "start": "npm run dev",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf dist",
    "env:check": "node -e \"require('dotenv').config(); console.log(process.env)\""
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "netlify-cli": "^17.10.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
